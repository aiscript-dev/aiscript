{
	"name": "@syuilo/aiscript",
	"version": "0.11.1",
	"description": "AiScript implementation",
	"author": "syuilo <syuilotan@yahoo.co.jp>",
	"license": "MIT",
	"repository": "https://github.com/syuilo/aiscript.git",
	"bugs": "https://github.com/syuilo/aiscript/issues",
	"main": "./built/index.js",
	"types": "./built/index.d.ts",
	"type": "module",
	"scripts": {
		"start": "node ./run",
		"parse": "node ./parse",
		"peg": "peggy --format es --cache -o src/parser/parser.mjs --allowed-start-rules Entry,Core --optimize size src/parser/parser.peggy && npm run peg-copy",
		"peg-copy": "copyfiles -f src/parser/parser.mjs built/parser/",
		"build": "npm run peg && tsc",
		"api": "npx api-extractor run --local --verbose",
		"api-prod": "npx api-extractor run --verbose",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"jest": "jest --coverage --detectOpenHandles",
		"tsd": "tsd",
		"test": "npm run jest"
	},
	"devDependencies": {
		"@microsoft/api-extractor": "7.18.4",
		"@types/jest": "26.0.24",
		"@types/node": "16.4.7",
		"@types/seedrandom": "2.4.28",
		"@typescript-eslint/eslint-plugin": "4.28.5",
		"@typescript-eslint/parser": "4.28.5",
		"copyfiles": "2.4.1",
		"eslint": "7.31.0",
		"jest": "27.0.6",
		"peggy": "1.2.0",
		"ts-jest": "27.0.4",
		"ts-node": "10.1.0",
		"tsd": "0.17.0",
		"typescript": "4.3.5"
	},
	"dependencies": {
		"autobind-decorator": "2.4.0",
		"chalk": "4.1.1",
		"seedrandom": "3.0.5",
		"stringz": "2.1.0",
		"uuid": "7.0.3"
	}
}
